{
	"extends": "@mckayla/tsconfig",
	"compilerOptions": {
		"lib": ["dom", "esnext"],
		"allowJs": false,
		"skipLibCheck": false,
		"noEmit": true,
		"noImplicitOverride": false,
		"tsBuildInfoFile": "./.next/tsconfig.tsbuildinfo",
		"baseUrl": ".",
		"paths": {
			"^/*": ["./*"]
		}
	},
	"include": ["next-env.d.ts", "**/*"],
	"exclude": ["node_modules"]
}
